<h2>Sign up</h2>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
<p><%= f.label :actname, "The name of your act / troupe" %><br />
<%= f.text_field :actname %></p>

<p><%= f.label :name, "Your Name" %><br />
<%= f.text_field :name %></p>

  <div><%= f.label :email, "Email Address" %><br />
  <%= f.email_field :email %></div>

  <div><%= f.label :password, "Password" %><br />
  <%= f.password_field :password %></div>

  <div><%= f.label :password_confirmation, "Password (again for confirmation)" %><br />
  <%= f.password_field :password_confirmation %></div>

  <div><%= f.submit "Sign up" %></div>
<% end %>

<%= render "devise/shared/links" %>
