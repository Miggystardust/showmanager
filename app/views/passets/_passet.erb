	<TR>
		<TD ALIGN=CENTER>
			<% if passet.kind.match("Audio file") -%>
			<object type="application/x-shockwave-flash" data="/dewplayer/dewplayer-rect.swf" width="240" height="20" id="dewplayer" name="dewplayer">
				<param name="wmode" value="transparent" />
				<param name="movie" value="/dewplayer/dewplayer-rect.swf" /> 
				<param name="flashvars" value="mp3=/uploads/download/<%=passet.uuid%>&amp;showtime=1" /> 
			</object>
			<% else -%>
			<%= image_tag passet.thumb_path(100,100) %>
			<% end -%>
		</TD>
		<TD>
			<%=passet.filename%><BR>
      <%=passet.created_at.localtime().asctime%><BR>
      <i><%=time_ago_in_words(passet.created_at.localtime())%> ago</i>
		</TD>
		<TD>
    <%= button_to 'Add to Set', passet, :method=>:addtoset, :class=> :addtoset %>
		</TD>
 		<TD>
    <%= button_to 'Remove', passet, :method=>:delete, :class=>:destroy %>
		</TD>
	</TR>
